<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <import type="android.view.View"/>

        <variable
            name="viewModel"
            type="com.natallialemiasheuskaya.sreens.homework4.ClockViewModel">
        </variable>

    </data>
<FrameLayout
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.natallialemiasheuskaya.sreens.homework4.ClockActivity">


    <com.natallialemiasheuskaya.sreens.homework4.ClockView
        android:id="@+id/clockView"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_gravity="center">

    </com.natallialemiasheuskaya.sreens.homework4.ClockView>


    <ImageView
        android:id="@+id/owl_with_clock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center" />


</FrameLayout>
</layout>